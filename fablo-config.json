{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.2.0/schema.json",
  "global": {
    "fabricVersion": "2.5.4",
    "tls": true
  },
  "orgs": [
    {
      "organization": { "name": "KPU", "domain": "kpu.id" },
      "peer": { "instances": 1, "db": "CouchDb" }
    },
    {
      "organization": { "name": "Akademik", "domain": "univ.ac.id" },
      "peer": { "instances": 1, "db": "CouchDb" }
    },
    {
      "organization": { "name": "Orderer", "domain": "orderer.net" },
      "orderer": { "type": "solo", "instances": 1 }
    }
  ],
  "channels": [
    {
      "name": "pemilu-channel",
      "orgs": [
        { "name": "KPU", "peers": ["peer0"] },
        { "name": "Akademik", "peers": ["peer0"] }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "evoting-cc",
      "version": "1.0",
      "lang": "golang",
      "channel": "pemilu-channel",
      "directory": "./chaincodes/evoting"
    }
  ]
}